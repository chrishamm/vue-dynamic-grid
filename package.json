{
	"name": "vue-dynamic-grid",
	"description": "Enhanced grid layout component based on vue-grid-layout",
	"version": "1.0.2",
	"author": "Christian Hammacher <bmasterc@gmail.com>",
	"license": "MIT",
	"keywords": [
		"grid",
		"dynamic",
		"vuejs",
		"drag",
		"draggable",
		"resize",
		"resizable",
		"fluid",
		"responsive"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/chrishamm/vue-dynamic-grid.git"
	},
	"main": "dist/vue-dynamic-grid.common.js",
	"unpkg": "dist/vue-dynamic-grid.umd.min.js",
	"scripts": {
